<% provide(:title, "Log in") %>
<nav class="main_color">
  <ul class="ul_in_nav">
    <li><a class="page-link" href="/"><i class="fa arrow fa-arrow-left"></i></a></li>
    <li><a class="page-link" href="#"><h1>LOG IN</h1></a></li>
    <li><a class="page-link" href="#"><h3>Log in</h3></a></li>
  </ul>
</nav>
<div class="container">
  <div class="row">
    <div class="col-md-3">
    </div>
    <div class="col-md-5">
      <%= form_for(:session, url: login_path) do |f| %>
      <%= f.text_field :name, class: 'form-control', placeholder: "Full Name" %>
      <%= f.submit "Log in", class: "btn btn-primary main_color" %>
      <%end%>
      <p>New user? <%= link_to "Sign up now!", signup_path %></p>
    </div>
      <div class="col-md-3">
    </div>
  </div>
</div>


